{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
  "name": "mtdev/colaborations",
	"title": "Colaboraciones",
	"version": "0.1.0",
	"category": "widgets",
	"icon": "format-quote",
  "supports": {
    "html": false,
    "align": [ "wide", "full" ]
  },
  "attributes": {
    "align": {
      "type": "string"
    },
    "category": {
      "type": "number",
      "default": -1
    },
    "order": {
      "type": "string",
			"default": "desc"
		},
		"orderBy": {
      "type": "string",
			"default": "date"
		},
    "numberOfItems": {
      "type": "number",
      "default": 3
    }
  },
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}

